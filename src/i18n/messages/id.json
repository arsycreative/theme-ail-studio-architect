    "pillars": {
      "title": "Filosofi",
      "items": [
        {
          "title": "Proporsi Utama",
          "description": "Kami merujuk pada tatanan klasik dan batasan modern. Setiap ruang diseimbangkan sebelum diberi ornamen."
        },
        {
          "title": "Kejujuran Material",
          "description": "Batu tetap terasa batu; kayu tetap terasa kayu. Finishing dipilih agar menua dengan anggun."
        },
        {
          "title": "Cahaya Sebagai Material",
          "description": "Cahaya siang dibentuk, bukan ditambahkan. Bayangan memberi ukuran—kontras dijaga terkendali."
        },
        {
          "title": "Utilitas Tenang",
          "description": "Detil menghilang ke dalam fungsi. Kenyamanan dan akses servis dirancang, bukan ditempelkan."
        }
      ]
    },
