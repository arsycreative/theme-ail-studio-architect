  "about": {
    "metadataTitle": "About — AIL Studio",
    "hero": {
      "eyebrow": "About AIL Studio",
      "title": "A practice devoted to proportion, material, and light.",
      "description": "We work with restraint—designing calm, enduring spaces tailored to everyday life.",
      "image": "https://images.unsplash.com/photo-1616594039964-ae9021a400a0",
      "imageAlt": "Studio portrait"
    },
    "manifesto": {
      "title": "Manifesto",
      "paragraphs": [
        "We value discipline over spectacle. Rooms that feel inevitable; details that prefer to be felt rather than seen. Materials are chosen to wear softly, light is shaped to be generous, and spatial order is tuned to daily rituals.",
        "We are rigorous about serviceability—access panels, cleaning edges, and replacement logic are embedded from the start. Luxury is quiet when it truly supports living."
      ],
      "image": "https://images.unsplash.com/photo-1693578616322-c8abe6c7393d?ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&q=80&w=1740",
      "imageAlt": "Atelier desk"
    },
    "directors": {
      "title": "Directors",
      "subtitle": "Senior oversight on every project.",
      "items": [
        {
          "name": "A. Wirawan",
          "role": "Principal Architect",
          "img": "https://images.unsplash.com/photo-1620668233692-c83e9563f337?ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&q=80&w=774",
          "bio": "Leads architectural vision with emphasis on disciplined proportion, daylight, and serviceable details."
        },
        {
          "name": "N. Prameswari",
          "role": "Design Director, Interiors",
          "img": "https://images.unsplash.com/photo-1738739907559-8998f9e6e626?ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&q=80&w=774",
          "bio": "Orchestrates interior palettes and joinery systems—warm tactility, calm tonality, and enduring comfort."
        }
      ]
    },
    "pillars": {
      "title": "Philosophy",
      "items": [
        {
          "title": "Proportion First",
          "description": "We draw from classical orders and modern constraints. Each room balanced before embellishment."
        },
        {
          "title": "Material Honesty",
          "description": "Stone feels like stone; timber like timber. Finishes are chosen for aging gracefully."
        },
        {
          "title": "Light as a Material",
          "description": "Daylight is shaped, not added. Shadow gives measure—the contrast stays restrained."
        },
        {
          "title": "Quiet Utility",
          "description": "Details disappear into function. Comfort and service access are designed, not appended."
        }
      ]
    },
    "approach": {
      "title": "Approach",
      "description": "From discovery to post-occupancy tuning, we keep one clear thread: proportion, material, and light aligned with daily use.",
